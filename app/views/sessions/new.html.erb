<h3>Login</h3>
<%= form_for(:session, url: login_path) do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <br />
  <br />
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br />
  <br />
  <%= f.submit "Log in!" %>
<% end %>

<p>
  New user? <%= link_to "Sign up now!", signup_path %>
</p>
